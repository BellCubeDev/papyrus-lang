{
    "activationEvents": [
        "onLanguage:papyrus",
        "onLanguage:papyrus-project"
    ],
    "author": {
        "email": "joelday@gmail.com",
        "name": "Joel Day",
        "url": "https://github.com/joelday"
    },
    "bugs": {
        "url": "https://github.com/joelday/papyrus-lang/issues"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "commands": [
            {
                "command": "papyrus.compile",
                "title": "Papyrus: Compile"
            },
            {
                "command": "papyrus.compiledebug",
                "title": "Papyrus: Compile (Debug)"
            },
            {
                "command": "papyrus.compilerelease",
                "title": "Papyrus: Compile (Release)"
            },
            {
                "command": "papyrus.compilefinal",
                "title": "Papyrus: Compile (Final)"
            },
            {
                "command": "papyrus.createproject",
                "title": "Papyrus: Create Project"
            }
        ],
        "configuration": {
            "properties": {
                "papyrus.fallout4.creationKitIniFiles": {
                    "default": [
                        "CreationKit.ini",
                        "CreationKitCustom.ini"
                    ],
                    "type": "array"
                },
                "papyrus.fallout4.installPath": {
                    "default": "C:\\Program Files (x86)\\Steam\\steamapps\\common\\Fallout 4\\",
                    "type": "string"
                },
                "papyrus.fallout4.compiler.asm": {
                    "type": "string",
                    "default": "Discard",
                    "description": "Specify the Papyrus Compiler assembly mode. \n\"None\" disables all output, \"Only\" only generates Papyrus assembly, not compiled .pex, \"Discard\" generates .pex and deletes assembly, and \"Keep\" generates and keeps both .pex and assembly.",
                    "enum": [
                        "None",
                        "Keep",
                        "Only",
                        "Discard"
                    ]
                },
                "papyrus.fallout4.compiler.mode": {
                    "type": "string",
                    "default": "Debug",
                    "description": "placeholder",
                    "enum": [
                        "Debug",
                        "Release",
                        "Release Final"
                    ]
                },
                "papyrus.skyrim.creationKitIniFiles": {
                    "default": [
                        "SkyrimEditor.ini"
                    ],
                    "type": "array"
                },
                "papyrus.skyrim.installPath": {
                    "default": "C:\\Program Files (x86)\\Steam\\steamapps\\common\\Skyrim\\",
                    "type": "string"
                },
                "papyrus.skyrim.compiler.asm": {
                    "type": "string",
                    "default": "Discard",
                    "description": "Specify the Papyrus Compiler assembly mode. \n\"None\" disables all output, \"Only\" only generates Papyrus assembly, not compiled .pex, \"Discard\" generates .pex and deletes assembly, and \"Keep\" generates and keeps both .pex and assembly.",
                    "enum": [
                        "None",
                        "Keep",
                        "Only",
                        "Discard"
                    ]
                },
                "papyrus.skyrim.compiler.mode": {
                    "type": "string",
                    "default": "Debug",
                    "description": "placeholder",
                    "enum": [
                        "Debug",
                        "Release",
                        "Release Final"
                    ]
                },
                "papyrus.skyrimSpecialEdition.creationKitIniFiles": {
                    "default": [
                        "CreationKit.ini",
                        "CreationKitCustom.ini"
                    ],
                    "type": "array"
                },
                "papyrus.skyrimSpecialEdition.installPath": {
                    "default": "C:\\Program Files (x86)\\Steam\\steamapps\\common\\Skyrim Special Edition\\",
                    "type": "string"
                },
                "papyrus.skyrimSpecialEdition.compiler.asm": {
                    "type": "string",
                    "default": "Discard",
                    "description": "Specify the Papyrus Compiler assembly mode. \n\"None\" disables all output, \"Only\" only generates Papyrus assembly, not compiled .pex, \"Discard\" generates .pex and deletes assembly, and \"Keep\" generates and keeps both .pex and assembly.",
                    "enum": [
                        "None",
                        "Keep",
                        "Only",
                        "Discard"
                    ]
                },
                "papyrus.skyrimSpecialEdition.compiler.mode": {
                    "type": "string",
                    "default": "Debug",
                    "description": "placeholder",
                    "enum": [
                        "Debug",
                        "Release",
                        "Release Final"
                    ]
                }
            },
            "title": "Papyrus",
            "type": "object"
        },
        "grammars": [
            {
                "language": "papyrus",
                "path": "./syntaxes/papyrus/papyrus.tmLanguage",
                "scopeName": "source.papyrus"
            },
            {
                "language": "papyrus-project",
                "path": "./syntaxes/papyrus-project/papyrus-project.json",
                "scopeName": "text.xml.papyrus-project"
            }
        ],
        "languages": [
            {
                "aliases": [
                    "Papyrus"
                ],
                "configuration": "./syntaxes/papyrus/papyrus-language-configuration.json",
                "extensions": [
                    ".psc"
                ],
                "id": "papyrus"
            },
            {
                "aliases": [
                    "Papyrus Project"
                ],
                "configuration": "./syntaxes/papyrus-project/papyrus-project-language-configuration.json",
                "extensions": [
                    ".ppj"
                ],
                "id": "papyrus-project"
            }
        ],
        "keybindings": [
            {
                "command": "papyrus.compile",
                "key": "Ctrl+B",
                "when": "editorTextFocus && editorLangId == papyrus"
            }
        ],
        "views": {
            "explorer": [
                {
                    "id": "papyrus-lang-vscode.astTreeView",
                    "name": "Papyrus Syntax Tree",
                    "when": "papyrus-lang-vscode.astTreeView:enabled"
                }
            ]
        }
    },
    "contributors": [
        {
            "email": "joelday@gmail.com",
            "name": "Joel Day",
            "url": "https://github.com/joelday"
        },
        {
            "email": "ScrivenerMods@gmail.com",
            "name": "Scrivener07",
            "url": "https://github.com/Scrivener07"
        },
        {
            "name": "shad0wshayd3",
            "url": "https://github.com/shad0wshayd3"
        }
    ],
    "dependencies": {
        "ini": "^1.3.5",
        "ps-list": "^6.0.0",
        "vscode-languageclient": "^5.1.0",
        "xml-writer": "^1.7.0"
    },
    "description": "Advanced language support for Fallout 4's Papyrus scripting language.",
    "devDependencies": {
        "@types/ini": "^1.3.30",
        "@types/mocha": "^5.2.6",
        "@types/node": "^11.9.4",
        "cpr": "^3.0.1",
        "prettier": "^1.16.4",
        "rimraf": "^2.6.3",
        "tslint": "^5.12.1",
        "typescript": "^3.3.3",
        "vscode": "^1.1.29"
    },
    "displayName": "Papyrus",
    "engines": {
        "vscode": "^1.27.0"
    },
    "homepage": "https://github.com/joelday/papyrus-lang/tree/master/packages/papyrus-lang-vscode",
    "icon": "images/icon-small.png",
    "keywords": [
        "Papyrus",
        "Fallout 4",
        "Scripting",
        "Modding",
        "Creation Kit"
    ],
    "main": "./out/extension",
    "maintainers": [
        {
            "email": "joelday@gmail.com",
            "name": "Joel Day",
            "url": "https://github.com/joelday"
        }
    ],
    "name": "papyrus-lang-vscode",
    "preview": true,
    "private": true,
    "publisher": "joelday",
    "repository": {
        "type": "git",
        "url": "https://github.com/joelday/papyrus-lang.git"
    },
    "scripts": {
        "copy-bin": "rimraf ./bin && cpr ../DarkId.Papyrus.Host/bin/ ./bin/ -o",
        "clean": "rimraf out",
        "compile": "tsc -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "vscode:prepublish": "npm run compile",
        "watch": "tsc -watch -p ./"
    },
    "version": "1.2.0",
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "arrowParens": "always",
        "printWidth": 120
    }
}
