{
    "activationEvents": [
        "onLanguage:papyrus",
        "onLanguage:papyrus-project"
    ],
    "author": {
        "email": "joelday@gmail.com",
        "name": "Joel Day",
        "url": "https://github.com/joelday"
    },
    "bugs": {
        "url": "https://github.com/joelday/papyrus-lang/issues"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "grammars": [
            {
                "language": "papyrus",
                "path": "./syntaxes/papyrus/papyrus.json",
                "scopeName": "source.papyrus"
            },
            {
                "language": "papyrus-project",
                "path": "./syntaxes/papyrus-project/papyrus-project.json",
                "scopeName": "text.xml.papyrus-project"
            }
        ],
        "languages": [
            {
                "aliases": [
                    "Papyrus"
                ],
                "configuration": "./syntaxes/papyrus/papyrus-language-configuration.json",
                "extensions": [
                    ".psc"
                ],
                "id": "papyrus"
            },
            {
                "aliases": [
                    "Papyrus Project"
                ],
                "configuration": "./syntaxes/papyrus-project/papyrus-project-language-configuration.json",
                "extensions": [
                    ".ppj"
                ],
                "id": "papyrus-project"
            }
        ],
        "snippets": [
            {
                "language": "papyrus",
                "path": "./snippets/papyrus/papyrus-classes.json"
            },
            {
                "language": "papyrus",
                "path": "./snippets/papyrus/papyrus-comments.json"
            },
            {
                "language": "papyrus",
                "path": "./snippets/papyrus/papyrus-statements.json"
            },
            {
                "language": "papyrus-project",
                "path": "./snippets/papyrus-project/papyrus-project.json"
            }
        ]
    },
    "contributors": [
        {
            "email": "ScrivenerMods@gmail.com",
            "name": "Scrivener07",
            "url": "https://github.com/Scrivener07"
        },
        {
            "name": "shad0wshayd3",
            "url": "https://github.com/shad0wshayd3"
        }
    ],
    "dependencies": {
        "chokidar": "^2.0.4",
        "papyrus-lang": "^0.4.0",
        "papyrus-lang-lsp-server": "^0.4.0",
        "vscode-languageclient": "^5.0.1"
    },
    "description": "Advanced language support for Fallout 4's Papyrus scripting language.",
    "devDependencies": {
        "@types/chokidar": "^1.7.5",
        "@types/glob": "^5.0.35",
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.7.1",
        "rimraf": "^2.6.2",
        "typescript": "^3.0.1",
        "vscode": "^1.1.21"
    },
    "displayName": "Papyrus",
    "engines": {
        "vscode": "^1.26.0"
    },
    "homepage": "https://github.com/joelday/papyrus-lang/tree/master/packages/papyrus-lang-vscode",
    "icon": "images/icon-small.png",
    "keywords": [
        "Papyrus",
        "Fallout 4",
        "Scripting",
        "Modding",
        "Creation Kit"
    ],
    "main": "./lib/extension",
    "maintainers": [
        {
            "email": "joelday@gmail.com",
            "name": "Joel Day",
            "url": "https://github.com/joelday"
        }
    ],
    "name": "papyrus-lang-vscode",
    "preview": true,
    "private": true,
    "publisher": "joelday",
    "repository": {
        "type": "git",
        "url": "https://github.com/joelday/papyrus-lang.git"
    },
    "scripts": {
        "postinstall": "lerna run vscode:install --scope papyrus-lang-vscode",
        "vscode:install": "node ./node_modules/vscode/bin/install"
    },
    "version": "0.4.0"
}
